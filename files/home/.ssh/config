# Recommended to edit for actual device in use

Host github github.com azura tail9f1d8
IdentityFile ~/.ssh/djo-personal
IdentitiesOnly yes
AddKeysToAgent yes

Host github github.com stash
ControlPath ~/.ssh/control-%h-%p-%r
ControlMaster auto
ControlPersist yes
ServerAliveInterval 30

# Specific configuration for akatosh to handle wakeonlan scenarios
Host akatosh
  IdentityFile ~/.ssh/djo-personal
  IdentitiesOnly yes
  AddKeysToAgent yes
  # Disable control master for akatosh to avoid stale connection issues
  ControlMaster no
  # Add timeouts to prevent hanging
  ConnectTimeout 10
  ServerAliveInterval 30
  ServerAliveCountMax 3
  # Retry connection if it fails
  ConnectionAttempts 3
  # Use compression for better performance over Tailscale
  Compression yes

